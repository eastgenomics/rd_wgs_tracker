SELECT
  dbo_X_GO_GLH_ACTIVITY_REPORT.[WGS Referral ID],
  Max(dbo_X_GO_GLH_ACTIVITY_REPORT.[Test Order Priority]) 
    AS Priority,
  Min(dbo_X_GO_GLH_ACTIVITY_REPORT.[Order Request Date Time]) 
    AS [Order Date],
  Max(dbo_X_GO_GLH_ACTIVITY_REPORT.[Patient MRN]) 
    AS [One of family MRNs],
  Min(dbo_X_GO_GLH_ACTIVITY_REPORT.[Submitter Name]) 
    AS Submitter
FROM
  dbo_X_GO_GLH_ACTIVITY_REPORT
WHERE
  dbo_X_GO_GLH_ACTIVITY_REPORT.[Test Validation Status]
      Not In ('Cancelled', 'Pending Final Verification')
  AND 
    dbo_X_GO_GLH_ACTIVITY_REPORT.[Last Final Verify Date]
      Is Null
  AND
    dbo_X_GO_GLH_ACTIVITY_REPORT.[Beaker Procedure Name]
      = "RARE DISEASE WGS REPORT"
GROUP BY
  dbo_X_GO_GLH_ACTIVITY_REPORT.[WGS Referral ID]
HAVING
  dbo_X_GO_GLH_ACTIVITY_REPORT.[WGS Referral ID] Is Not Null
  AND
    Min(dbo_X_GO_GLH_ACTIVITY_REPORT.[Submitter Name])
      Like "*Nottingham*"
ORDER BY
  Max(dbo_X_GO_GLH_ACTIVITY_REPORT.[Test Order Priority]) DESC,
  Min(dbo_X_GO_GLH_ACTIVITY_REPORT.[Order Request Date Time]);
