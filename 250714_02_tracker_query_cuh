SELECT
  tracker.Priority,
  tracker.[Order Date],
  tracker.[WGS Referral ID],
  tracker.[One of family MRNs],
  tracker.Submitter,
  tracker.Status,
  tracker.Batch
FROM
  tracker
WHERE
  (
    (tracker.Submitter) Not like "*Nottingham*")
    OR ((tracker.Submitter) Is Null)
  )
AND 
  (
    ((tracker.Status) Not In (2,5,6,7))
    OR ((tracker.Status) Is Null)
  )
ORDER BY
  tracker.Priority DESC,
  tracker.[Order Date];
